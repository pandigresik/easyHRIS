base: 
    cities
    companies
    departments
    regions
    settings

command ==> php artisan scaffold.all:generate --module=base --table=cities,companies,departments,regions,settings

accounting:
    tax_group_history
    taxs

command ==> php artisan scaffold.all:generate --module=accounting --table=tax_group_history,taxs

